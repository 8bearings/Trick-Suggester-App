  <h1 class="levelTitle">Starting Out</h1>
  <div class='peanut' include="form-input-select()">
    <select required onchange="location = this.value">
      <!--
        This is how we can do "placeholder" options.
        note: "required" attribute is on the select
      -->
      <option value=""
              hidden
      >Skill Level</option>
 
      <!-- normal options -->

      <option value="/start">Just Starting</option>
      <option value="/beginner">Beginner</option>
      <option value="/intermediate">Intermediate</option>
      <option value="/gnarly">Gnarly</option>
      
    </select>
  </div>

  <!-- Randomizer -->
  <div class='randoContain'>
    <div class='randoTitle'>
      <img src='https://images.vexels.com/media/users/3/199290/isolated/lists/a26521db2e9b3c3eabdb418cd45326e6-black-and-white-skateboard.png'/>
    </div>
    <div class='team home'>
      <h5>Trick</h5>
      <div class='logo'></div>
      <div class='name'></div>
      <div class='random'>Randomize</div>
    </div>
    <div class='clear'></div>
  </div>
  
