<h1 class="levelTitle">Gnarly!</h1>
  <div class='peanut' include="form-input-select()">
    <select required onchange="location = this.value">
      <!--
        This is how we can do "placeholder" options.
        note: "required" attribute is on the select
      -->
      <option value=""
              hidden
      >Skill Level</option>
 
      <!-- normal options -->

      <option value="/start">Just Starting(0/1)</option>
      <option value="/beginner">Beginner(2)</option>
      <option value="/intermediate">Intermediate(3)</option>
      <option value="/gnarly">Gnarly(4)</option>
      
    </select>
  </div>
   <!-- Randomizer -->
